# This file prepares rhino to do something useful

# Prep rhino
load('dist/env.rhino.js');
window.location='index.html';
load('dist/jquery.js');

# grab all the useful strings from a .html file
myTags = "h1, h2, h3, title, label, button";
$('meta').each(function(){ print($(this).attr('content'));})
$('img[alt]').each(function(){ print($(this).attr('alt'));})
$(myTags).each(function(){ print($(this).html())})

